{
  "name": "webpack5-ts-react",
  "version": "1.0.0",
  "description": "react ts 脚手架,最新版本",
  "main": "index.js",
  "author": "shawnxiao",
  "license": "MIT",
  "scripts": {
    "serve": "npx http-server dist",
    "start": "webpack serve --inline --progress --config build/webpack.config.js --mode development --env dev",
    "dev": "webpack --profile --json > stats.json --config build/webpack.config.js --color --progress",
    "build": "webpack --color --progress --config build/webpack.config.js --mode production --env prod",
    "test": "webpack --env test --config build/webpack.prod.js --color --progress",
    "build:dll": "webpack --config ./build/webpack.dll.js",
    "add": "add-component",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"{src,mock}/**/*.{ts,tsx,js,css,less}\"",
    "lint": "eslint src --ext .ts,.tsx && stylelint \"./src/**/*.less\"",
    "lint:fix": "eslint src --ext .ts,.tsx && stylelint \"./src/**/*.less\" --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "bourbon": "^7.0.0",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "core-js": "^3.8.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.16.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.0.8",
    "html-webpack-plugin": "^4.5.0",
    "http-server": "^0.12.3",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "mini-css-extract-plugin": "^1.3.2",
    "moment-locales-webpack-plugin": "^1.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "react-dev-utils": "^11.0.1",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "thread-loader": "^3.0.1",
    "typed-css-modules-webpack-plugin": "^0.2.0",
    "typescript": "^4.1.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.10.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^6.0.2",
    "yargs-parser": "^20.2.4"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}